<div class="row">
  <div class="col-sm">
    <h3 class="pt-2 pl-3">Editing Url for: <%= @pandemic.name %></h3>
    <div class="dropdown-divider"></div>
    <%= render partial: 'data_urls/form', locals: { data_url: [@pandemic, @data_url] } %>
    <div class="col">
      <%= link_to 'Show', pandemic_data_url_path(@pandemic, @data_url), class: "form-control btn btn-primary" %>
    </div>
    <div class="col">
      <%= link_to 'Back', pandemic_data_urls_path(@pandemic), class: "form-control btn btn-primary" %>
    </div>
  </div>
</div>
</div>
